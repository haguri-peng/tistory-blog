doctype html

html(lang="ko")
  head
    //- Tistory RSS
    link(rel="alternate" type="application/rss+xml" title="[##_title_##]" href="[##_rss_url_##]")

    //- meta
    meta(charset="utf-8")
    meta(name="viewport" content="width=device-width, initial-scale=1")
    meta(name="naver-site-verification" content="556bd12679a1cf8511189cc532b13600b0db7c10")

    title [##_page_title_##]

    //- scripts
    script(src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous")
    script(src="https://code.jquery.com/ui/1.12.1/jquery-ui.min.js" integrity="sha256-VazP97ZCwtekAsvgPBSUwPFKdrwD3unUfSGVYrahUqU=" crossorigin="anonymous")
    //- script(src="https://kit.fontawesome.com/095c7780e2.js" crossorigin="anonymous")
    script(src="https://platform.twitter.com/widgets.js")

    //- Google Adsense
    script(async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-6187932165258053" crossorigin="anonymous")

    //- Style Sheets
    link(rel="preconnect" href="https://fonts.googleapis.com")
    link(rel="preconnect" href="https://fonts.gstatic.com" crossorigin)
    link(rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Poor+Story&display=swap")
    link(rel="stylesheet" href="https://fonts.googleapis.com/earlyaccess/nanumgothiccoding.css")

    //- highlightjs
    //- link(rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/11.6.0/styles/base16/dracula.min.css")
    //- link(rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/11.6.0/styles/tomorrow-night-blue.min.css")
    //- script(src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/11.6.0/highlight.min.js")

    style.
      /* Color Palette */
      :root {
        --color-background: white;
        --color: black;
        --color-blur: rgba(0, 0, 0, .6)
      }
      @media (prefers-color-scheme: dark) {
        :root {
          --color-background: #1e1f21;
          --color: white;
          --color-blur: rgba(255, 255, 255, .8)
        }
      }

    style.
      @font-face {
        font-family: 'MaplestoryOTFLight';
        src: url('https://cdn.jsdelivr.net/gh/projectnoonnu/noonfonts_20-04@2.1/MaplestoryOTFLight.woff') format('woff');
        font-weight: normal;
        font-style: normal;
      }
      * {
        padding: 0px;
        margin: 0px;
        /*font-family: 'Poor Story', cursive;*/
        font-family: 'MaplestoryOTFLight', serif;
      }
      a {
        pointer-events: initial;
      }
      body {
        background-color: var(--color-background);
      }

  body(id="[##_body_id_##]")
    block TIDORY
